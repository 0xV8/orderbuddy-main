{
  "routes": [
    {
      "route": "/.well-known/apple-developer-merchantid-domain-association",
      "allowedRoles": ["anonymous"],
      "serve": "/.well-known/apple-developer-merchantid-domain-association",
      "serve": "/.well-known/apple-developer-merchantid-domain-association",

{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "always",
  "bracketSameLine": false,
  "trailingComma": "all",
  "useTabs": false,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "lf",
  "importOrder": ["react", "<THIRD_PARTY_MODULES>", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
